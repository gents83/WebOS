{
  "name": "webos-windows-like",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/main.css --minify",
    "build:js": "esbuild index.tsx --bundle --outfile=dist/main.js --format=esm --minify --sourcemap --loader:.png=file --loader:.jpg=file --loader:.svg=file --loader:.woff2=file --define:ngDevMode=false --define:ngJitMode=false",
    "build": "npm run build:css && npm run build:js"
  },
  "dependencies": {
    "@angular/common": "^20.3.12",
    "@angular/compiler": "^20.3.12",
    "@angular/core": "^20.3.12",
    "@angular/forms": "^20.3.12",
    "@angular/platform-browser": "^20.3.12",
    "@google/genai": "^1.29.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "esbuild": "^0.23.0"
  }
}
